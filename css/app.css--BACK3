  text-transform: uppercase;
  color: #FFF;
}
#menu ul li span a .navIcon {
  width: 50px;
  height: 60px;
  line-height: 60px;
  display: inline-block;
  margin-left: -4px;
  vertical-align: middle;
}
#menu ul li span a .navIcon svg {
