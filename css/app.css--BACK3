/* MAIN PAGE */
.page-wrap {
  position: relative;
  right: 0;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  -webkit-transition: all 0.3s ease-in;
  -moz-transition: all 0.3s ease-in;
  -o-transition: all 0.3s ease-in;
  transition: all 0.3s ease-in;
}

#menu-switch:checked ~ .page-wrap {
  right: 300px;
}

#menu-switch:checked ~ .page-wrap #top.minified {
  left: -300px !important;
}

/* MENU TOGGLE ICON */
#menu-toggle {
  display: block;
  position: fixed;
  float: right;
  width: 50px;
  height: 40px;
  z-index: 9999;
  top: 1.5rem;
  right: 2rem;
  cursor: pointer;
  border: none;
  text-align: center;
  -webkit-border-radius: 50px;
  -moz-border-radius: 50px;
  border-radius: 50px;
  -webkit-transition: all 0.5s ease;
  -moz-transition: all 0.5s ease;
  -o-transition: all 0.5s ease;
  transition: all 0.5s ease;
}

#menu-toggle:focus {
  outline: none;
}

#menu-toggle:before, #menu-toggle:after, #menu-toggle span {
  -webkit-transition: all 0.5s ease;
  -moz-transition: all 0.5s ease;
  -o-transition: all 0.5s ease;
  transition: all 0.5s ease;
}

#menu-toggle:before, #menu-toggle:after {
  position: absolute;
  content: "";
  background-color: #e46c5c;
}

#menu-toggle:before {
  top: 0px;
  right: 0;
  left: 0;
  width: 50px;
  height: 4px;
  border-radius: 6px;
}

#menu-toggle span {
  position: absolute;
  background-color: #e46c5c;
  top: 18px;
  right: 0;
  left: 0;
  width: 50px;
  height: 4px;
  opacity: 1;
  border-radius: 6px;
}

#menu-toggle:after {
  bottom: 0;
  right: 0;
  left: 0;
  width: 50px;
  height: 4px;
  border-radius: 6px;
}

#menu-switch:checked ~ .page-wrap #menu-toggle {
  right: 320px;
}

#menu-switch:checked ~ .page-wrap #menu-toggle:before {
  -webkit-transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  transform: rotate(45deg);
  top: 16px;
}

#menu-switch:checked ~ .page-wrap #menu-toggle span {
