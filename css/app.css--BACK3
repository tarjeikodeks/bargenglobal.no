#top {
  background-color: transparent;
  position: relative;
}
#top .navContainer {
  position: relative;
}
#top.minified {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 999;
  transition: all 0.3s ease;
  box-shadow: 0 0 20px rgba(0, 0, 0, 0.05);
  background-color: #e46c5c;
  border-bottom: 4px solid #FFF;
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);
}
#top.minified .branding .slogan {
  display: none;
}
#top.minified .logo {
  height: 60px;
  background-size: 183px 34px;
  transition: all 0.5s ease;
}
#top.minified .logo .mainLogo {
  margin-top: -60px;
  opacity: 0;
}
#top.minified .logo .minLogo {
  opacity: 1;
}
#top.minified .menuToggle {
  line-height: 60px;
  color: #fefefe;
}
#top.minified .mainNav {
  top: 60px;
}
#top.minified .mainNav ul li a {
  color: #fefefe;
}
#top.minified .mainNav ul li a:after {
  background-color: #f3f3f3;
}
#top.minified .mainNav.open ul li a {
  color: #e46c5c;
}
#top .branding .slogan {
  display: none;
  position: absolute;
  top: 110px;
  left: 1rem;
  max-width: 400px;
  font-size: 0.9rem;
  color: #fff;
  z-index: 999;
}
#top .branding .slogan a {
  color: #FFF;
}
#top .logo {
  display: block;
  height: 90px;
  overflow: hidden;
  transition: all 0.3s ease;
}
#top .logo .mainLogo {
  display: block;
  width: 90px;
  height: 90px;
  position: absolute;
  top: 1rem;
  z-index: 999;
  transition: all 0.3s ease;
  margin-top: 0;
  opacity: 1;
}
#top .logo .mainLogo img {
  max-width: 200px;
  height: auto;
  position: absolute;
  top: 0;
  bottom: 0;
  margin: auto;
}
#top .logo .minLogo {
  display: block;
  width: 100px;
  height: 100%;
  max-height: 300px;
  position: relative;
  transition: all 0.3s ease;
  opacity: 0;
}
#top .logo .minLogo img {
  max-height: 50%;
  width: auto;
  position: absolute;
  top: 0;
  bottom: 0;
  margin: auto;
}
#top input {
  margin-bottom: 0;
}

.menuToggle {
  font-size: 2rem;
